# PAI Environment Variables
# Copy this file to .env and fill in your values

# ElevenLabs TTS API Key (optional - for cloud voice)
ELEVENLABS_API_KEY=YOUR_ELEVENLABS_API_KEY

# Google API Key (for Gemini research agents)
GOOGLE_API_KEY=YOUR_GOOGLE_API_KEY

# Strava API Configuration (optional - for fitness tracking)
STRAVA_CLIENT_ID=YOUR_STRAVA_CLIENT_ID
STRAVA_CLIENT_SECRET=YOUR_STRAVA_CLIENT_SECRET

# Garmin Configuration (optional - for fitness tracking)
GARMIN_EMAIL=YOUR_GARMIN_EMAIL
GARMIN_PASSWORD=YOUR_GARMIN_PASSWORD
